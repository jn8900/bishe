			</tr>

<%
	String name,sex,birthday,password,tel,address,qq,question,answer,u_name; 
	int priority;
 	//打开数据库
 	String NAME = new String(request.getParameter("NAME").getBytes("ISO8859_1"));
	String sql="select * from user where priority='1' and name='"+NAME+"'";
 	ResultSet rs = userbean.executeQuery(sql);
 
	out.print("<center>");

 	while(rs.next())
	{
  		name = rs.getString(2);
		sex = rs.getString(3);
		birthday = rs.getString(4);
		password = rs.getString(5);
		tel = rs.getString(6);
		address = rs.getString(7);
		qq = rs.getString(8);
		question = rs.getString(9);
		answer = rs.getString(10);
		u_name = rs.getString(11);
		priority = rs.getInt(12);
%>
		
<%
  		out.print("<table>"); 
  		out.print("<tr><td width='120' height='20'>"+name+"</td><td width='80' height='20'>"+sex+"</td><td width='100'  height='20'>"+birthday+"</td><td width='130'  height='20'>"+password+"</td><td width='140'  height='20'>"+tel+"</td><td width='100'  height='20'>"+address+"</td><td width='100'  height='20'>"+qq+"</td><td width='100'  height='20'>"+question+"</td><td width='100'  height='20'>"+answer+"</td><td width='100'  height='20'>"+u_name+"</td><td width='60'  height='20'>"+priority+"</td></tr>");
